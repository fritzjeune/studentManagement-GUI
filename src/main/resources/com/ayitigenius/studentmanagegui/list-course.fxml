<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ayitigenius.studentmanagegui.management.CourseList">
   <children>
      <VBox layoutX="15.0" layoutY="49.0" prefHeight="287.0" prefWidth="571.0">
         <children>
            <TableView fx:id="courseTable" VBox.vgrow="ALWAYS">
              <columns>
                  <TableColumn fx:id="courseCode" prefWidth="75.0" text="Course Code" />
                  <TableColumn fx:id="courseName" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="maxCapacity" prefWidth="75.0" text="Maximum Capacity" />
                  <TableColumn fx:id="totalEnrolledStudent" prefWidth="75.0" text="Total Enrolled Student" />
<!--                  <TableColumn fx:id="EnrolledCourses" prefWidth="75.0" text="Enrolled Courses" />-->
              </columns>
            </TableView>
         </children>
      </VBox>
      <Text layoutX="15.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="List Of Registered Student">
         <font>
            <Font name="Berlin Sans FB" size="18.0" />
         </font>
      </Text>
      <Button layoutX="503.0" layoutY="360.0" mnemonicParsing="false" text="Add Student" />
   </children>
</AnchorPane>

